@charset "utf-8";

.visa-info{
  width: 1150px;
  padding: 20px;
  border: 1px solid #e3e3e3;
  margin-bottom: 20px;
}
.visa-info .visa-info-top{
  border-bottom: 1px dashed #e3e3e3;
  margin-bottom: 20px;
  padding-bottom: 20px;
}
.visa-info .visa-info-top .visa-img{
  width: 197px;
  height: 117px;
  border: 1px solid #e3e3e3;
  float: left;
}
.visa-info .visa-info-top .visa-img img{
  width: 197px;
  height: 117px;
}
.visa-info .visa-info-top .visa-content{
  width: 740px;
  float: left;
  margin-left: 15px;
}
.visa-info .visa-info-top .visa-content h1{
  font-size: 18px;
  color: #333;
  font-weight: normal;
  margin:5px 0 10px;
}
.visa-info .visa-info-top .visa-content > ul > li{
  float: left;
  width: 350px;
  padding-right: 20px;
  font-size: 14px;
  line-height: 24px;
  color: #666;
}
.visa-info .visa-info-top .visa-price{
  float: right;
  width: 160px;
  text-align: right;
}
.visa-info .visa-info-top .visa-price p{
  font-size: 36px;
  color: #f60;
  margin: 10px 0 5px;
}
.visa-info .visa-info-top .visa-price .btn{
  font-size: 16px;
  line-height: 30px;
}
.visa-info .visa-info-bottom{
  font-size: 14px;
  line-height: 24px;
  color: #999;
}
.visa-info .visa-info-bottom p span{
  color: #666;
}

.visa-box{
  border: 1px solid #e3e3e3;
  padding: 20px 0;
  margin-bottom: 20px;
}
.visa-box-title{
  font-size: 18px;
  height: 24px;
  line-height: 24px;
  color: #333;
  border-left: 5px solid #f60;
  padding-left: 15px;
}

.visa-pad{
  margin-top: 20px;
  padding:0 20px;
}
.inventory-tab > ul{
  height: 42px;
}
.inventory-tab > ul > li{
  width: 168px;
  font-size: 14px;
  float: left;
  line-height: 42px;
  text-align: center;
  cursor: pointer;
  color: #333;
  background: #f5f5f5;
  position: relative;
}
.tabico{
  display: inline-block;
  background: url(../images/tab-ico.png) no-repeat;
  vertical-align: middle;
  position: absolute;
}
.ico-right{
  width: 15px;
  height: 42px;
  background-position: 0 0;
  top: 0;
  right: 0;
  z-index: 10;
}
.ico-left{
  width: 15px;
  height: 42px;
  top: 0;
  left: -15px;
  z-index: 5;
}
.ico-last-right{
  width: 15px;
  height: 42px;
  background-position: 0 0;
  top: 0;
  right: -15px;
}
.inventory-tab > ul{
  margin-bottom: 20px;
}
.inventory-tab > ul > li.active{
  color: #fff;
  background: #f60;
}
.inventory-tab > ul > li.active .ico-left{
  background-position: -30px 0;
}
.inventory-tab > ul > li.active .ico-right{
  background-position: -15px 0;
}
.inventory-tab > ul > li.active .ico-last-right{
  background-position: -45px 0;
}

.inventory-con{
  font-size: 14px;
  color: #666;
  display: none;
}
.inventory-con.on{
  display: block;
}
.inventory-table{
  border: 1px solid #e3e3e3;
  width: 100%;
  font-size: 14px;
  color: #666;
  line-height: 28px;
  text-align: center;
}
.inventory-table td{
  padding: 8px 15px;
  border-bottom: 1px solid #e3e3e3;
  border-right: 1px solid #e3e3e3;
}
.text-left{
  text-align: left;
}
.text-left a{
  color: #f60;
  text-decoration: underline;
  margin-left: 20px;
}
.tab-img{
  max-width: 100%;
}

.declare-content{
  font-size: 14px;
  line-height: 24px;
  color: #999;
}