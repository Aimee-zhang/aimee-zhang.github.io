.cjwt{padding:0 20px 0 20px;border:1px solid #e3e3e3;}
.cjwt li{list-style:none;border-bottom:1px dashed #e3e3e3;height:50px;overflow:hidden;line-height:50px;font-size:14px;}
.cjwt li a{color:#8a8a8a;text-decoration:none;}
.cjwt li.hide-border{border-bottom:none;}
