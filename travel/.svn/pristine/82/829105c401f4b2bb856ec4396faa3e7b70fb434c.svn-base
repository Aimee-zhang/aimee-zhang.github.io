.cjwt-xq{padding:0 20px 100px 20px;border:1px solid #e3e3e3;}
.cjwt-xq h1{line-height:65px;font-size:20px;color:#666;border-bottom:1px dashed #e3e3e3;}
.cjwt-xq-nr-list h4{font-size:14px;color:#666;padding-top:15px;}
.cjwt-xq-nr-list p{color:#8a8a8a;padding-top:3px;} 
