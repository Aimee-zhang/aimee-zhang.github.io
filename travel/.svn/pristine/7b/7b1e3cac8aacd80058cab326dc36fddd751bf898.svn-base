.qzzc{padding:0 13px 13px 13px;border:1px solid #e3e3e3;}
.qzzc ul li{list-style:none;border-bottom:1px dashed #f0f0f0;padding:20px 0;}
.qzzc ul li:last-child{border-bottom:none;padding-bottom:0;}
.qzzc ul li h4{font-size:18px;color:#666;}
.qzzc-center{position:relative;padding-top:20px;}
.qzzc-center-dw{width:230px;height:115px;position:absolute;left:0;top:20px;}
.qzzc-center-dw img{width:100%;height:100%;}
.qzzc-center-right{padding-left:245px;height:115px;}
.qzzc-center-nr{height:85px;overflow:hidden;font-size:14px;color:#8a8a8a;line-height:28px;}
.qzzc-center-nr a{color:#ea5404;}
.qzzc-time{height:30px;font-size:14px;color:#bbbbbb;line-height:30px;}
/**签证政策-详情**/
.bod{padding:0 20px 20px 20px;border:1px solid #e3e3e3;}
.bod-time{border-bottom:1px dashed #e3e3e3;}
.bod-time h1{line-height:55px;font-size:20px;color:#666;}
.bod-time p{padding:10px 0 20px 0;font-size:14px;color:#bbb;}
.bod-time p span{padding:0 12px 0 30px;}
.bod-time p em{padding-left:5px;border-left:1px solid #bbb;}
.himg{width:100%;padding:25px 0;text-align:center}
.himg img{max-width:758px;}
.ggnr{width:100%;}
.ggnr p{color:#8a8a8a;font-size:14px;padding-bottom:20px;}
.pre{padding-top:20px;border-top:1px dashed #e3e3e3;line-height:25px;font-size:14px;color:#8a8a8a;}
.pre a{color:#8a8a8a;}
.pre p{height:30px;}
.pre span{padding-left:50px;}



