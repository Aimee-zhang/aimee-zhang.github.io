.qzzc-wt-list{padding:15px;}
.qzzc-wt-list ul{overflow:hidden;}
.qzzc-wt-list li{list-style:none;width:328px;height:27px;line-height:27px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;font-size:14px;}
.qzzc-wt-list li a{color:#8a8a8a;text-decoration:none;}
.qzzc-ys-list{margin:0 15px;}
.qzzc-ys-list ul{padding-top:30px;}
.qzzc-ys-list li{list-style:none;border-bottom:1px dashed #e5e5e5;display:table;width:100%;padding:15px 0;}
.qzzc-ys-list li .wmys{padding-left:100px;position:relative;}
.qzzc-ys-list li .ico{width:34px;height:52px;background-position:-35px -295px;position:absolute;left:52px;top:-8px;}
.qzzc-ys-list dl{color:#666;}
.qzzc-ys-list li dt{font-size:18px;}
.qzzc-ys-list li dd{font-size:14px;}
.qzzc-ys-list li.border-none{border-bottom:none;}
.qzzc{padding:0 13px 13px 13px;border:1px solid #e3e3e3;}
.qzzc ul li{list-style:none;border-bottom:1px dashed #f0f0f0;padding:20px 0;}
.qzzc ul li:last-child{border-bottom:none;padding-bottom:0;}
.qzzc ul li h4{font-size:18px;color:#666;}
.qzzc-center{position:relative;padding-top:20px;}
.qzzc-center-dw{width:230px;height:115px;position:absolute;left:0;top:20px;}
.qzzc-center-dw img{width:100%;height:100%;}
.qzzc-center-right{padding-left:245px;height:115px;}
.qzzc-center-nr{height:85px;overflow:hidden;font-size:14px;color:#8a8a8a;line-height:28px;}
.qzzc-center-nr a{color:#ea5404;}
.qzzc-time{height:30px;font-size:14px;color:#bbbbbb;line-height:30px;}


