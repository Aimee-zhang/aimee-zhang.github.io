.qgqz-list{width:1190px;}
.qgqz-list-left{width:358px;border:1px solid #e3e3e3;margin-right:20px;padding-top:15px;}
.qgqz-left-title{border-left:4px solid #ff6600;padding-left:10px;line-height:24px;font-size:18px;color:#333;}
.qgqz-left-main{margin:30px 18px 0 18px;position:relative;}
.qgqz-main-left{position:absolute;top:0;left:0;width:13px;background:url(../images/qgqz-list/qgqz-list.png);margin:0 36px;height:100%;}
.qgqz-main-left img{display:block;margin-left:auto;margin-right:auto;width:13px;}
.qgqz-main-right{padding-left:86px;position:relative;margin-bottom:64px;}
.qgqz-main-right-img{position:absolute;top:0;left:0;}
.qgqz-main-right-img img{width:84px;height:84px;margin:1;}
.qgqz-main-right-nr{padding-left:15px;font-size:14px;}
.qgqz-main-right-nr span{display:block;width:100%;line-height:22px;color:#999;}
.qgqz-main-right-nr p{color:#666;}
.qgqz-list-right{width:810px;}
.qgqz-right-list{width:100%;height:100%;}
.qgqz-right-list li{list-style:none;padding:15px;border:1px solid #e3e3e3;margin-bottom:15px;}
.li_left{width:605px;border-right:1px dashed #e3e3e3;}
.li_left h4{color:#333;font-size:18px;}
.li_left dl{padding:15px 0;}
.li_left dl dt{display:inline-block;padding:8px 10px;background:#f5f5f5;color:#666;margin-right:10px;border-radius:6px;font-size:14px;}
.li_left p{color:#666;font-size:14px;line-height:22px;}
.li_right{width:172px;height:138px;display:table;}
.li_right div{vertical-align:middle;display:table-cell;text-align:center}
.li_right h3{font-size:36px;color:#ff6600;margin-bottom:10px;}
.li_right a{color:#666;padding:10px 30px;font-size:14px;background:#f5f5f5;border-radius:6px;}
