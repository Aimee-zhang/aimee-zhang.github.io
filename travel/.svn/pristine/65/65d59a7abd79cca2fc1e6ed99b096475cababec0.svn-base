/* 按钮 */
.btn{
  display:inline-block;
  font-size: 14px;
  line-height: 20px;
  padding: 5px 25px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  letter-spacing: 2px;
}

/* 绿色 */
.btn-green{
  background: #30b288;
  color: #fff;
}
.btn-green:hover{
  color: #fff;
}

/* 黄色 */
.btn-yellow{
  background: #f60;
  color: #fff;
}
.btn-yellow:hover{
  color: #fff;
}

/* 灰色 */
.btn-gray{
  background: #f5f5f5;
  color: #666;
}
.btn-gray:hover{
  color: #666;
}

/* ico图标 */
.spliceico{
  display: inline-block;
  background: url(../images/ico.png) no-repeat;
  vertical-align: middle;
}

