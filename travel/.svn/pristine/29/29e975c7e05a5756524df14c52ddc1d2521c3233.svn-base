  color: #999;
  margin-left: 10px;
  padding-bottom: 2px;
  border-bottom: 1px solid #e3e3e3;
}
.ico-modify{
  width: 16px;
  height: 16px;
  background-position: 0 -420px;
  margin-right: 2px;
}
.visa-detail-box{
  border-bottom: 1px solid #e3e3e3;
}
.visa-detail-list{
