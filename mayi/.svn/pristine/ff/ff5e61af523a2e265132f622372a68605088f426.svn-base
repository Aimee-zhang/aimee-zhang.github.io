.mb{margin-bottom:390px;animation:mb 0.5s linear;-webkit-animation:mb 0.5s linear;animation-fill-mode:forwards;-webkit-animation-fill-mode:forwards;}
.mt{margin-top:390px;animation:mt 0.5s linear;animation-fill-mode:forwards;-webkit-animation:mt 0.5s linear;-webkit-animation-fill-mode:forwards;}
@keyframes mb{
	0%{margin-top:-390px;}
	100{margin-top:0;}
}
