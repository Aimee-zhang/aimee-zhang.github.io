/***
 * 全局样式
 **/
p,body,span,a,i,em,ul,li,h1,h2,h3,h4,h5,h6{margin:0;padding:0;font-style:normal;font-family:"微软雅黑";}
body{background: #f4f4f4;}
.fl{float:left;}
.fr{float:right;}
ul,li{ list-style: none;  padding:0; margin:0; }
a{ color: #333;}
body{background:#2a3243;}
.hide{display:none;}
.clearfix{clear:both;}

