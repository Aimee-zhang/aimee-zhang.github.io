/** 精品课程 **/
.page-course-top{position:absolute;top: 0;left:0;right:0;overflow: hidden;}
.page-course-top img{width:100%;height: 100%;}


.page-activity .page-course-top{height:60%;}
.page-course .page-course-top{height:75%;}
.page-about .page-course-top{height:75%;}

.page-course-yuan{opacity:0.8;width:200px;height:200px;border-radius:50%;background:#2a3243;position:absolute;top:30px;left:30px;text-align: center;line-height:60px;animation:bounceIn 1.5s linear;-webkit-animation:bounceIn 1.5s linear;}
/*@keyframes c-bounceIn{
	0{opacity:0;}
	100%{opacity:0.8;}
}
@-webkit-keyframes c-bounceIn{
	0%{opacity:0;}
	100%{opacity:0.8;}
}*/

.page-course-yuan .page-course-yuan1{width:160px;margin:40px auto;line-height:60px;font-size:26px;height:120px;color:#fff;}
.page-course-yuan .page-course-yuan1 h1{font-size:30px;font-weight: 100;border-bottom: 1px solid #fff;line-height:60px;}

.page-course-bottom{position:absolute;bottom:0px;left:0;right:0;animation:bounceInUp 1.5s linear;-webkit-animation:bounceInUp 1.5s linear;}
/*@keyframes c-bot{
	0%{bottom:-300px;}
	100%{bottom:0;}
}
@-webkit-keyframes c-bot{
	0{bottom:-300px;}
	100%{bottom:0;}
}*/

.page-course-bottom .page-course-bg{width: 100%;position: relative;background:#2a3243;}
.page-course-bottom .page-course-bg:before{display: block;content:"";width:0;height:0;border-left:8px solid transparent;border-right:8px transparent solid;border-bottom:8px solid #2a3243;position: absolute;top:-7px;left:50%; transform:translateX(-50%);-webkit-transformX:translate(-50%); }
.page-course-bottom .mune{clear: both;overflow: hidden;padding:10px 0;}
.page-course-bottom .mune li{float:left; width:50%; padding-top:10px;text-align:center;}
.page-course-bottom .mune li img{width:95px;height:98px;}
.page-course-bottom .mune li a{font-size: 26px;}
.page-course-bottom .mune li a span{ display:block; padding:6px 0;color:#f1be59;}

.activity-con{color:#f1be59;padding:100px 15px;font-size:26px;}
.activity-con p{font-size:26px;margin-top:15px;}
.page-course-bottom .activity-mune{padding:20px 0 0;}
.page-course-bottom .activity-mune li{float:left;  margin:0 5%; width:22%; padding-top:10px;text-align:center;}
.page-course-bottom .activity-mune li img{width:97px;height:71px;}

.page-about-con{color:#f1be59;padding:120px 30px;font-size:32px;}
.page-about-con p{font-size:28px;margin-top:15px;line-height:44px;}

